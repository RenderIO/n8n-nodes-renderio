{
    "name": "@renderio/n8n-nodes-renderio",
    "version": "0.0.2",
    "description": "n8n community node for RenderIO - run FFmpeg commands in the cloud",
    "license": "MIT",
    "homepage": "https://renderio.dev",
    "keywords": [
        "n8n-community-node-package",
        "n8n-nodes",
        "n8n",
        "ffmpeg",
        "renderio"
    ],
    "author": {
        "name": "RenderIO",
        "email": "support@renderio.dev"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/RenderIO/n8n-nodes-renderio.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "test": "jest --config jest.config.js",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/RenderioApi.credentials.js",
            "dist/credentials/RenderioOAuth2Api.credentials.js"
        ],
        "nodes": [
            "dist/nodes/Renderio/Renderio.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "@types/jest": "^29.5.14",
        "eslint": "9.32.0",
        "jest": "^29.7.0",
        "nock": "^14.0.5",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "ts-jest": "^29.3.2",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    },
    "publishConfig": {
        "access": "public"
    }
}
